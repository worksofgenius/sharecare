<div class="container col-6">
  <div class="card-booking">
    <h2>How Can You Share?</h2>
    <div class="card-booking-info">
      <div>
        <%= simple_form_for [@listing] do |f| %>
          <%= f.input :title %>
          <%= f.input :service_description %>
          <%= f.input :tag_list, placeholder: "Separate tags with comma..." %>
          <%= f.input :location %>
          <%= f.input :fee %>
          <%= f.input :photo, as: :file %>
          <%= f.submit class: 'book-action-btn' %>
        <% end %>
      </div>
    </div>
  </div>
</div>