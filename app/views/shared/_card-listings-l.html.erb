  <% @listings.each do |listing| %>
  <div class="card-listing-l">
    <div class="listing-image">
      <%= image_tag "https://images.unsplash.com/photo-1596464716127-f2a82984de30?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1650&q=80", class: "listing-image" %>
      <div class="profile d-flex">
        <%= image_tag "https://images.unsplash.com/photo-1603903918985-f0cacbd0b3d9?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=2134&q=80", class: "profile-img" %>
        <div class="profile-name"><%= listing.user.first_name %>
        </div>
      </div>
    </div>
    <div class="listing-info">
      <div class="location">
        <p><%= listing.location %></p>
      </div>
      <div class="listing-title">
        <p>Title</p>
      </div>
      <div class="listing-title"><%= listing.title %></div>
      <div class="service_description">
        <%= listing.service_description %>
      </div>
      <div class="listing-fee">
        <p><%= listing.fee %></p>
        <%= link_to 'See details', class: "btn btn-primary" %>
      </div>
    </div>
  </div>
<% end %>